  dialogueLines:
  - characterName: "\u65C1\u767D"
    dialogueText: "\u4E3B\u89D2\u4F86\u5230\u6751\u9577\u5BB6\uFF0C\u6E96\u5099\u8A62\u554F\u8A73\u60C5\u3002\u591C\u8272\u6F38\u6DF1\uFF0C\u6751\u9577\u795E\u60C5\u56B4\u8085\u5730\u8FCE\u63A5\u4E86\u4ED6\u3002"
    characterSprite: {fileID: 0}
    sfx: {fileID: 0}
  - characterName: "\u6751\u9577"
    dialogueText: "\u4F60\u597D\uFF0C\u4F60\u5C31\u662F\u9019\u6B21\u88AB\u6D3E\u4F86\u8ABF\u67E5\u7684\u8B66\u5B98\u5427\uFF1F\u771F\u662F\u592A\u597D\u4E86\u2026\u2026\u6211\u5011\u90FD\u5FEB\u6490\u4E0D\u4E0B\u53BB\u4E86\u3002"
    characterSprite: {fileID: -1628682612147464315, guid: ee5ffe6f5183a1c478e54e2a4c58e7b4, type: 3}
    sfx: {fileID: 0}
  - characterName: "\u6751\u9577"
    dialogueText: "......\u5176\u5BE6\u5728\u4F60\u4E4B\u524D\uFF0C\u4E5F\u6709\u4EBA\u4F86\u904E\u3002"
    characterSprite: {fileID: -1628682612147464315, guid: ee5ffe6f5183a1c478e54e2a4c58e7b4, type: 3}
    sfx: {fileID: 0}
  - characterName: "\u4E3B\u89D2"
    dialogueText: "\u2026\u2026\u4F60\u662F\u8AAA\uFF0C\u4E4B\u524D\u4E5F\u6709\u4EBA\u8ABF\u67E5\u904E\u9019\u4EF6\u4E8B\uFF1F"
    characterSprite: {fileID: 8785289583145846344, guid: 6483f0fbf6a794844a7b3b2496dfba50, type: 3}
    sfx: {fileID: 0}
  - characterName: "\u6751\u9577"
    dialogueText: "\u6C92\u932F\u3002\u4F46\u4ED6\u5011\u6700\u5F8C\u90FD\u5687\u5F97\u5306\u5306\u96E2\u958B\u3002\u6709\u4EBA\u9084\u4E0D\u505C\u5583\u5583\u81EA\u8A9E\uFF1A\u300C\u90A3\u4E0D\u662F\u666E\u901A\u751F\u7269\u2026\u2026\u90A3\u4E0D\u662F\u666E\u901A\u751F\u7269\u2026\u2026\u300D"
    characterSprite: {fileID: -1628682612147464315, guid: ee5ffe6f5183a1c478e54e2a4c58e7b4, type: 3}
    sfx: {fileID: 0}
  - characterName: "\u6751\u9577"
    dialogueText: "\u6C92\u932F\u3002\u4F46\u4ED6\u5011\u6700\u5F8C\u90FD\u5687\u5F97\u5306\u5306\u96E2\u958B\u3002\u6709\u4EBA\u9084\u4E0D\u505C\u5583\u5583\u81EA\u8A9E\uFF1A\u300C\u90A3\u4E0D\u662F\u666E\u901A\u751F\u7269\u2026\u2026\u90A3\u4E0D\u662F\u666E\u901A\u751F\u7269\u2026\u2026\u300D"
    characterSprite: {fileID: -1628682612147464315, guid: ee5ffe6f5183a1c478e54e2a4c58e7b4, type: 3}
    sfx: {fileID: 0}
  - characterName: "\u4E3B\u89D2\uFF08\u5FC3\u60F3\uFF09"
    dialogueText: "\u2026\u2026\u96E3\u9053\u771F\u7684\u662F\u4EC0\u9EBC\u5947\u602A\u7684\u751F\u7269\u5728\u4F5C\u795F\uFF1F\u70BA\u4EC0\u9EBC\u6C92\u6709\u4EBA\u63D0\u8D77\u904E\uFF1F"
    characterSprite: {fileID: 8785289583145846344, guid: 6483f0fbf6a794844a7b3b2496dfba50, type: 3}
    sfx: {fileID: 0}
  - characterName: "\u4E3B\u89D2"
    dialogueText: "\u6211\u525B\u525B\u6CE8\u610F\u5230\uFF0C\u6751\u88E1\u4E0D\u5C11\u4EBA\u624B\u4E0A\u90FD\u6709\u56B4\u91CD\u6293\u75D5\u3002\u9019\u548C\u90A3\u500B\u751F\u7269\u6709\u95DC\u55CE\uFF1F"
    characterSprite: {fileID: 8785289583145846344, guid: 6483f0fbf6a794844a7b3b2496dfba50, type: 3}
    sfx: {fileID: 0}
  - characterName: "\u6751\u9577"
    dialogueText: "\u662F\u7684\u3002\u9019\u707D\u96E3\u4F3C\u4E4E\u8207\u52D5\u7269\u6709\u95DC\uFF0C\u4F46\u81F3\u4ECA\u627E\u4E0D\u5230\u6E90\u982D\u3002\u85E5\u4E5F\u7121\u6548\uFF0C\u6293\u7662\u7684\u60C5\u6CC1\u65E5\u76CA\u56B4\u91CD\u2026\u2026"
    characterSprite: {fileID: -1628682612147464315, guid: ee5ffe6f5183a1c478e54e2a4c58e7b4, type: 3}
    sfx: {fileID: 0}
  - characterName: "\u6751\u9577"
    dialogueText: "\u6211\u5011\u53EA\u80FD\u4F9D\u9760\u4F60\u4E86\uFF0C\u8B66\u5B98\u3002"
    characterSprite: {fileID: -1628682612147464315, guid: ee5ffe6f5183a1c478e54e2a4c58e7b4, type: 3}
    sfx: {fileID: 0}
  - characterName: "\u4E3B\u89D2"
    dialogueText: " \u6211\u6703\u8ABF\u67E5\u6E05\u695A\u7684\u3002"
    characterSprite: {fileID: 8785289583145846344, guid: 6483f0fbf6a794844a7b3b2496dfba50, type: 3}
    sfx: {fileID: 0}
  - characterName: "\u6751\u9577"
    dialogueText: "\u592A\u597D\u4E86\u2026\u2026\u73FE\u5728\u6642\u9593\u4E5F\u665A\u4E86\uFF0C\u6211\u5148\u5E36\u4F60\u53BB\u4F11\u606F\uFF0C\u660E\u5929\u518D\u7E7C\u7E8C\u8AC7\u5427\u3002"
    characterSprite: {fileID: -1628682612147464315, guid: ee5ffe6f5183a1c478e54e2a4c58e7b4, type: 3}
    sfx: {fileID: 0}
